{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "e-commerce website using nodeJs and mongoDB",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
